{
  "name": "udagram-image-filter",
  "version": "1.0.0",
  "description": "A Node-Express application which runs a simple script to process images.",
  "main": "./www/server.js",
  "scripts": {
    "start": "node ./www/server.js",
    "tsc": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
