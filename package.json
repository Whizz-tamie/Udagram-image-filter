  "bugs": {
    "url": "https://github.com/Whizz-tamie/udagram-image-filter/issues"
  },
  "homepage": "https://github.com/Whizz-tamie/udagram-image-filter#readme",
  "dependencies": {
    "node": "^16.15.0",
    "express": "^4.18.1",
    "jimp": "^0.16.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "image-url-validator": "^1.0.4",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.3"
